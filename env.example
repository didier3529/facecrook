# Copy this file to .env and update the values
# API Configuration
REACT_APP_API_URL=http://localhost:3001
REACT_APP_API_BASE_URL=http://localhost:3001/api

# OpenAI Configuration
REACT_APP_OPENAI_KEY=your_openai_api_key_here

# AWS S3 Configuration
REACT_APP_AWS_S3_BUCKET=your_s3_bucket_name
REACT_APP_AWS_ACCESS_KEY_ID=your_aws_access_key
REACT_APP_AWS_SECRET_ACCESS_KEY=your_aws_secret_key
REACT_APP_AWS_REGION=us-east-1

# IPFS Configuration
REACT_APP_IPFS_ENDPOINT=https://ipfs.infura.io:5001
REACT_APP_IPFS_PROJECT_ID=your_ipfs_project_id
REACT_APP_IPFS_PROJECT_SECRET=your_ipfs_project_secret

# Application Settings
REACT_APP_APP_NAME=FaceCrook
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Socket.io Configuration
REACT_APP_SOCKET_URL=http://localhost:3001

# MetaMask/Web3 Configuration
REACT_APP_CHAIN_ID=1
REACT_APP_NETWORK_NAME=mainnet 